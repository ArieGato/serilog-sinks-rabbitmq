{
    "vhosts":[ 
        { 
            "name": "/" 
        } 
    ],
    "users": [
        {
            "name": "guest",
            "password": "guest",
            "tags": "administrator"
        }
    ],
    "permissions": [
        {
            "user": "guest",
            "vhost": "/",
            "configure": ".*",
            "write": ".*",
            "read": ".*"
        }
    ],
    "exchanges": [
        {
            "name": "serilog-sink-exchange",
            "vhost": "/",
            "type": "fanout",
            "durable": true,
            "auto_delete": false,
            "internal": false,
            "arguments": {}
        }
    ],
    "queues": [
        {
            "name": "serilog-sink-queue",
            "vhost": "/",
            "durable": true,
            "auto_delete": false,
            "arguments": {}
        }
    ],
    "bindings": [
        {
            "source": "serilog-sink-exchange",
            "vhost": "/",
            "destination": "serilog-sink-queue",
            "destination_type": "queue",
            "routing_key": "*",
            "arguments": {}
        }
    ]
  }